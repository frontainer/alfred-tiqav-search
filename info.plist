				<key>script</key>
				<string>require_once('workflows.php');
$wf = new Workflows();

define('API_PATH','http://api.tiqav.com/search.json?q=');
$orig = "{query}";

$content = json_decode($wf-&gt;request(API_PATH.urlencode( $orig )));

$int = 1;
$cache = $wf-&gt;cache();

foreach( $content as $val ):
	$id = $val-&gt;id;
	$ext = $val-&gt;ext;
	$src = "http://img.tiqav.com/$id.th.jpg";
	$icon_curl = curl_init();
	curl_setopt( $icon_curl, CURLOPT_URL, $src );
	curl_setopt( $icon_curl, CURLOPT_RETURNTRANSFER, true );
	$data = curl_exec( $icon_curl );
	$icon = $cache.$id.'jpg';

	$query = "http://img.tiqav.com/$id.$ext";
	file_put_contents($icon, $data);
	$wf-&gt;result($int.'.'.time(), $query, $src, $id, $icon);
	$int++;
endforeach;

$results = $wf-&gt;results();
if ( count( $results ) == 0 ):
	$wf-&gt;result( 'ちくわぶ', $orig, 'No Image', 'No search image found. Search ちくわぶ for '.$orig, 'icon.png' );
endif;

echo $wf-&gt;toxml();</string>
